"vim.wikia.com/wiki/Example_vimrc

colorscheme default

set nocompatible
filetype indent plugin on
syntax on
set t_Co=16

set wildmenu
set showcmd
set hlsearch
set nomodeline
set backspace=indent,eol,start
set autoindent
set ruler
set laststatus=2
set confirm
set visualbell
set cmdheight=2
set number

" set shiftwidth=2
" set softtabstop=2
" set expandtab

set shiftwidth=2
set tabstop=2

